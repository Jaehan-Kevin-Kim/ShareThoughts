  "compilerOptions": {
    "esModuleInterop": true,
    "sourceMap": true,
    "lib": [
      "ES2020",
      "DOM"
    ],
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "Node",
    "target": "es5",
    "strict": false,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@hooks/*": [
        "hooks/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@typings/*": [
        "typings/*"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "incremental": true,
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
